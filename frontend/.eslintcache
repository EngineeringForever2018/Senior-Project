av/NavBar\";\r\nimport {InstructorHome} from \"./instructor/InstructorHome\";\r\nimport {StudentHome} from \"./student/StudentHome\";\r\nimport {getUserData} from \"./requests\";\r\n\r\nexport function Home() {\r\n  const {isAuthenticated, getAccessTokenSilently} = useAuth0()\r\n\r\n  const history = useHistory()\r\n  const location = useLocation()\r\n\r\n  if (isAuthenticated) {\r\n    let userData = undefined;\r\n\r\n    if (location.state !== undefined) {\r\n      userData = location.state.userData\r\n    }\r\n\r\n    if (userData === undefined) {\r\n      getAccessTokenSilently().then((token) => {\r\n        getUserData(token).then((response) => {\r\n          history.push('/', {userData: response.data})\r\n        }, (error) => console.log(error.response))\r\n      })\r\n    } else {\r\n      if (userData['user_type'] === 'student') {\r\n        return <StudentHome userData={userData}/>\r\n      } else {\r\n        return <InstructorHome userData={userData}/>\r\n      }\r\n    }\r\n  }\r\n\r\n  return UnauthenticatedHome()\r\n}\r\n\r\nfunction UnauthenticatedHome() {\r\n  return (\r\n    <div>\r\n      <UnauthenticatedNavBar/>\r\n    </div>\r\n  )\r\n}\r\n\r\n","/home/gchristensen/base/repos/avpd/frontend/src/instructor/InstructorHome.js",["87","88","89","90"],"/home/gchristensen/base/repos/avpd/frontend/src/requests.js",[],"/home/gchristensen/base/repos/avpd/frontend/src/student/StudentHome.js",["91","92","93","94"],"\r\nimport {NavBar} from \"../nav/NavBar\";\r\n\r\nimport React, {useEffect, useState} from \"react\";\r\n\r\nimport {useAuth0} from \"@auth0/auth0-react\";\r\nimport {useHistory} from \"react-router-dom\";\r\nimport axios from \"axios\";\r\nimport {createEssay, getAssignments, viewAssignment} from \"../requests\";\r\n\r\nexport function StudentHome(props) {\r\n  return (\r\n    <div>\r\n      <NavBar firstName={props.userData['first_name']} lastName={props.userData['last_name']}/>\r\n      <Essay />\r\n    </div>\r\n  )\r\n}\r\n\r\n//load all essays\r\nfunction Essay() {\r\n  const {getAccessTokenSilently} = useAuth0();\r\n  const history = useHistory()\r\n  const [essaysTitles, setEssaysTitles] = useState();\r\n  const [classNumber, setClassNumber] = useState(10);\r\n  const [currentAssignment, setCurrentAssignment] = useState();\r\n\r\n  function handleSetClassNumber(e) {\r\n    setClassNumber(e.target.value);\r\n    console.log(classNumber)\r\n\r\n    getAccessTokenSilently().then((token) => {\r\n      getAssignments(token, classNumber).then((response) => {\r\n        setEssaysTitles(\r\n          response.data.map((data) => <li>\r\n            <button onClick={() => {\r\n              viewAssignment(data['id'], classNumber, token).then(() => history.push('/'), (error) => console.log(error.response))\r\n            }}>\r\n              {data['title']}\r\n            </button>\r\n          </li>)\r\n        )\r\n      })\r\n    })\r\n  }\r\n\r\n  useEffect(() => {\r\n    getAccessTokenSilently().then((token) => {\r\n      getAssignments(token, classNumber).then((response) => {\r\n        setEssaysTitles(\r\n          response.data.map((data) => <li>\r\n            <button onClick={() => {\r\n              viewAssignment(data['id'], classNumber, token).then(() => history.push('/'), (error) => console.log(error.response))\r\n            }}>\r\n              {data['title']}\r\n            </button>\r\n          </li>)\r\n        )\r\n      })\r\n    })\r\n  }, [])\r\n\r\n  return (\r\n    <div>\r\n      <form>\r\n        <div>\r\n          <label>Class number input: </label>\r\n          <select defaultValue={classNumber} onChange={handleSetClassNumber}>\r\n            <option value = \"1\" > class 1 </option>\r\n            <option value = \"2\"> class 2 </option>\r\n            <option value = \"3\"> class 3 </option>\r\n            <option value = \"10\"> class 10 </option>\r\n          </select>\r\n        </div>\r\n      </form>\r\n      <div>\r\n        list of assignments\r\n        {essaysTitles}\r\n      </div>\r\n\r\n      Submit assignment\r\n      <button onClick={() => {\r\n        history.push('/submit-essay')\r\n      }}\r\n      >Create Essay\r\n      </button>\r\n    </div>\r\n  )\r\n}\r\n\r\n//handler for submit\r\nexport function CreateEssayForm() {\r\n  const [title, setTitle] = useState(\"\");\r\n  const [file, setFile] = useState(\"\");\r\n  const {getAccessTokenSilently} = useAuth0();\r\n\r\n  const history = useHistory()\r\n\r\n  function handleChange(event) {\r\n    setTitle(event.target.value);\r\n    setFile(event.target.value);\r\n  }\r\n\r\n  function handleSubmit(event) {\r\n    getAccessTokenSilently().then((token) => {\r\n      createEssay(title, file, token).then((response) => {\r\n        history.push('/')\r\n      }, (error) => console.log(error.response))\r\n    })\r\n\r\n    event.preventDefault()\r\n  }\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit}>\r\n      <label>\r\n        Title:\r\n        <input type=\"text\" value={title} onChange={handleChange}/>\r\n      </label>\r\n      <div>\r\n        Submit file:\r\n        <input type=\"file\" value={file}  onChange={handleChange}/>\r\n      </div>\r\n      <input type=\"submit\" value=\"Submit\"/>\r\n    </form>\r\n  )\r\n}","/home/gchristensen/base/repos/avpd/frontend/src/utils.js",["95"],"import {useAuth0} from \"@auth0/auth0-react\";\r\n\r\nexport function serverUrl() {\r\n  return process.env.REACT_APP_SERVER_URL;\r\n}\r\n\r\nexport async function authenticateRequest(requestConfig, token) {\r\n  requestConfig.headers = {\r\n    'Authorization': `Bearer ${token}`\r\n  }\r\n\r\n  return requestConfig\r\n}","/home/gchristensen/base/repos/avpd/frontend/src/About.js",[],"/home/gchristensen/base/repos/avpd/frontend/src/instructor/ReportScreen.js",[],"/home/gchristensen/base/repos/avpd/frontend/src/instructor/Classroom.js",["96","97"],"/home/gchristensen/base/repos/avpd/frontend/src/instructor/Assignment.js",["98","99"],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","repl