 </button>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport function CreateClassroomForm() {\r\n  const [title, setTitle] = useState(\"\");\r\n  const {getAccessTokenSilently} = useAuth0();\r\n\r\n  const history = useHistory()\r\n\r\n  function handleChange(event) {\r\n    setTitle(event.target.value);\r\n  }\r\n\r\n  function handleSubmit(event) {\r\n    getAccessTokenSilently().then((token) => {\r\n      createClassroom(title, token).then((response) => {\r\n        history.push('/')\r\n      }, (error) => console.log(error.response))\r\n    })\r\n\r\n    event.preventDefault()\r\n  }\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit}>\r\n      <label>\r\n        Title:\r\n        <input type=\"text\" value={title} onChange={handleChange}/>\r\n      </label>\r\n      <input type=\"submit\" value=\"Submit\"/>\r\n    </form>\r\n  )\r\n}","/home/gchristensen/base/repos/avpd/frontend/src/requests.js",[],"/home/gchristensen/base/repos/avpd/frontend/src/student/StudentHome.js",[],"/home/gchristensen/base/repos/avpd/frontend/src/utils.js",["77"],"import {useAuth0} from \"@auth0/auth0-react\";\r\n\r\nexport function serverUrl() {\r\n  return process.env.REACT_APP_SERVER_URL;\r\n}\r\n\r\nexport async function authenticateRequest(requestConfig, token) {\r\n  requestConfig.headers = {\r\n    'Authorization': `Bearer ${token}`\r\n  }\r\n\r\n  return requestConfig\r\n}","/home/gchristensen/base/repos/avpd/frontend/src/About.js",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"78","replacedBy":"82"},{"ruleId":"80","replacedBy":"83"},{"ruleId":"78","replacedBy":"84"},{"ruleId":"80","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":3,"column":8,"nodeType":"88","messageId":"89","endLine":3,"endColumn":13},{"ruleId":"86","severity":1,"message":"87","line":4,"column":8,"nodeType":"88","messageId":"89","endLine":4,"endColumn":13},{"ruleId":"90","severity":1,"message":"91","line":37,"column":6,"nodeType":"92","endLine":37,"endColumn":8,"s